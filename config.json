{
  "threads": 30,
  "max_retries": 5,
  "timeout": 20,
  "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64)",
  
  "proxy_pool": "https://api.proxyscrape.com/v2/?request=getproxies&protocol=https",
  "proxy_auth": null,
  "proxy_country": "US",
  
  "db_config": {
    "host": "localhost",
    "port": 3306,
    "user": "root",
    "password": "your_password",
    "db": "image_downloader",
    "charset": "utf8mb4"
  },
  
  "content_filter": {
    "min_size": [1200, 800],
    "max_size": [3840, 2160],
    "content_types": [
      "image/jpeg",
      "image/png",
      "image/webp"
    ]
  },
  
  "logging": {
    "level": "INFO",
    "file": "logs/image_downloader.log",
    "backup_count": 7,
    "max_bytes": 10485760
  }
}
